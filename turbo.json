{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    "tsconfig.json",
    "$NODE_ENV",
    "$ACCESS_TOKEN_PRIVATE_KEY",
    "$ACCESS_TOKEN_PUBLIC_KEY",
    "$REFRESH_TOKEN_PRIVATE_KEY",
    "$REFRESH_TOKEN_PUBLIC_KEY"
  ]
}
